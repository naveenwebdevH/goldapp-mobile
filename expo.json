{
  "expo": {
    "name": "GoldApp",
    "slug": "goldapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goldapp.mobile"
    },
    "android": {
      "package": "com.goldapp.mobile"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
