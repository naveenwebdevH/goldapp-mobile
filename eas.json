{
  "cli": {
    "version": ">= 3.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "EXPO_ANDROID_COMPILE_SDK_VERSION": "35",
          "EXPO_ANDROID_TARGET_SDK_VERSION": "35"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
